{
  "name": "iam",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "features/iam/src",
  "prefix": "pisces",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["features/iam/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "features/iam/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/features/iam",
        "main": "features/iam/src/main-backend.ts",
        "tsConfig": "features/iam/tsconfig.app.json",
        "assets": ["features/iam/src/resource/assets"],
        "isolatedConfig": true,
        "webpackConfig": "packages/backend/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "iam:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "iam:build:development"
        },
        "production": {
          "buildTarget": "iam:build:production"
        }
      }
    }
  },
  "tags": []
}
