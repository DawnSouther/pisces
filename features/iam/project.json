{
  "name": "iam",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "features/iam/src",
  "prefix": "pisces",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "features/iam/**/*.ts",
          "features/iam/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "features/iam/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "build-backend": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/features/iam",
        "main": "features/iam/src/main-backend.ts",
        "tsConfig": "features/iam/tsconfig.app.json",
        "assets": ["features/iam/src/resource"],
        "isolatedConfig": true,
        "webpackConfig": "packages/backend/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve-backend": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "iam:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "iam:build-backend:development"
        },
        "production": {
          "buildTarget": "iam:build-backend:production"
        }
      }
    },
    "build-frontend": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/features/iam",
        "index": "libs/frontend/src/index.html",
        "main": "features/iam/src/main-frontend.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "features/iam/tsconfig.app.json",
        "assets": [
          "features/iam/src/assets"
        ],
        "allowedCommonJsDependencies": [
          "base64-js",
          "lodash",
          "moment",
          "photoviewer"
        ],
        "styles": ["libs/frontend/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-frontend": {
      "executor": "@nx/angular:webpack-dev-server",
      "configurations": {
        "production": {
          "browserTarget": "iam:build-frontend:production"
        },
        "development": {
          "browserTarget": "iam:build-frontend:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "browserTarget": "features/iam:build-frontend",
        "host": "0.0.0.0",
        "port": 4300,
        "hmr": true,
        "disableHostCheck": true,
        "proxyConfig": "libs/frontend/proxy.config.js"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "frontend:build"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "frontend:build"
      }
    }
  },
  "tags": []
}
