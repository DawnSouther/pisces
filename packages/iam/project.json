{
  "name": "iam",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/iam/src",
  "prefix": "pisces",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/iam/**/*.ts",
          "packages/iam/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/iam/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "build-backend": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/packages/iam",
        "main": "packages/iam/src/main-backend.ts",
        "tsConfig": "packages/iam/tsconfig.backend.app.json",
        "assets": [
          "packages/iam/src/resource"
        ],
        "isolatedConfig": true,
        "webpackConfig": "libs/backend/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve-backend": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "iam:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "iam:build-backend:development"
        },
        "production": {
          "buildTarget": "iam:build-backend:production"
        }
      }
    },
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/iam",
        "index": "libs/frontend/src/index.html",
        "browser": "packages/iam/src/main-frontend.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "packages/iam/tsconfig.frontend.app.json",
        "assets": [
          "packages/iam/src/assets"
        ],
        "allowedCommonJsDependencies": [
          "base64-js",
          "lodash",
          "moment"
        ],
        "styles": [
          "libs/frontend/src/styles.scss"
        ],
        "scripts": [],
        "server": "packages/iam/src/main.server.ts",
        "prerender": false,
        "ssr": {
          "entry": "packages/iam/server.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "iam:build:production"
        },
        "development": {
          "buildTarget": "iam:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "packages/iam:build",
        "host": "0.0.0.0",
        "port": 4300,
        "hmr": true,
        "disableHostCheck": true,
        "proxyConfig": "libs/frontend/proxy.config.js"
      }
    },
    "test-serve": {
      "executor": "@pisces/executor:union-application",
      "configurations": {
        "production": {
          "buildTarget": "iam:build-frontend:production"
        },
        "development": {
          "buildTarget": "iam:build-frontend:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "packages/iam:build-frontend",
        "host": "0.0.0.0",
        "port": 4300,
        "hmr": true,
        "disableHostCheck": true,
        "proxyConfig": "libs/frontend/proxy.config.js"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "frontend:build"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "frontend:build"
      }
    }
  },
  "tags": []
}
